<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.redpacket.activity.mapper.SpringBootTestMapper">
    <insert id="insertSpringBootTestInfo" parameterType="com.redpacket.activity.bean.SpringBootTestBean">
        INSERT INTO spring_boot_test (tvid, redpacked_id, type, c_time)
        VALUES (#{tvid},#{redpackedId},#{type},#{cTime})
    </insert>
</mapper>